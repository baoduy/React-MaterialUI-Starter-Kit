{
  "name": "react-materialui-started-kit",
  "version": "0.0.1",
  "main": "src/index.jsx",
  "description": "Starter kit for React, Material-UI, Webpack (with Hot Module Replacement) and Babel.",
  "keywords": [
    "react",
    "webpack",
    "babel",
    "less",
    "hmr",
    "starter",
    "boilerplate",
    "material-ui",
    "dashboard"
  ],
  "author": "Duy Hoang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baoduy/React-MaterialUI-Started-Kit.git"
  },
  "bugs": {
    "url": "https://github.com/baoduy/React-MaterialUI-Started-Kit/issues"
  },
  "homepage": "https://github.com/baoduy/React-MaterialUI-Started-Kit",
  "scripts": {
    "git:config": "git submodule update --init configs",
    "build": "webpack -p --progress --config=configs/webpack/prod",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run lint:js && npm run lint:less",
    "lint:js": "jshint",
    "lint:less": "lesshint ./src/assets/less",
    "start": "npm run start-dev",
    "start-dev": "npm run git:config && webpack-dev-server --config=configs/webpack/dev",
    "start-prod": "npm rum git:config && npm run build && node sm-react-node-express/express.js",
    "test": "jest --watch --coverage --config=configs/jest/jest.json",
    "test:ci": "jest --ci --coverage --config=configs/jest/jest.json && codecov -t 75b11d17-22d7-4b30-81b1-4c39fa2f7582",
    "check": "npm-check ignores='eslint,babel-*'"
  },
  "devDependencies": {
    "@types/prop-types": "^15.5.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.5",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class": "0.3.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react-latest": "6.1.0",
    "babel-preset-stage-3": "6.24.1",
    "compression": "1.7.3",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "6.0.0",
    "img-loader": "^3.0.0",
    "jest": "23.4.1",
    "jshint": "^2.9.5",
    "less": "3.8.0",
    "less-loader": "^4.1.0",
    "lesshint": "^5.1.0",
    "postcss-loader": "2.1.6",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.3.3",
    "react-moment-proptypes": "^1.6.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "4.16.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@material-ui/core": "1.4.0",
    "@material-ui/icons": "1.1.0",
    "@types/googlemaps": "3.30.11",
    "@types/markerclustererplus": "2.1.33",
    "babel-runtime": "^6.26.0",
    "chartist": "0.11.0",
    "classnames": "2.2.6",
    "linq": "^3.1.0",
    "moment": "^2.22.2",
    "perfect-scrollbar": "1.4.0",
    "react": "^16.4.1",
    "react-chartist": "0.13.1",
    "react-dom": "^16.4.1",
    "react-google-maps": "^9.4.5",
    "react-redux": "^5.0.7",
    "react-redux-thunk-store": "0.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "redux-toolbelt": "2.1.5",
    "redux-toolbelt-thunk": "2.0.4",
    "request-promise": "^4.2.2",
    "uuid": "^3.3.2"
  },
  "postcss": {},
  "jest": {
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "collectCoverage": true
  }
}
